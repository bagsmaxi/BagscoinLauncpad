# Bagscreener Environment Variables
# Copy this file to .env and fill in your values

# Server Port (default: 3001)
PORT=3001

# Bags.fm API Key
# Get yours at: https://dev.bags.fm
BAGS_API_KEY=your_bags_api_key_here

# Helius RPC URL
# Get yours at: https://helius.dev
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_key_here
