# Bagscoin Launchpad Environment Variables
# Copy this file to .env and fill in your values

# Bags.fm API Key (get from dev.bags.fm)
BAGS_API_KEY=your_bags_api_key_here

# Helius RPC URL (get from helius.dev)
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key_here

# Optional: Custom port (default 3001)
# PORT=3001
