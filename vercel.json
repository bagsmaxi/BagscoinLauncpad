{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/bags/creator", "dest": "/api/bags/creator.js" },
    { "src": "/api/bags/claim-stats", "dest": "/api/bags/claim-stats.js" },
    { "src": "/api/bags/lifetime-fees", "dest": "/api/bags/lifetime-fees.js" },
    { "src": "/api/helius/rpc", "dest": "/api/helius/rpc.js" },
    { "src": "/(.*\\.css)", "dest": "/$1" },
    { "src": "/(.*\\.js)", "dest": "/$1" },
    { "src": "/", "dest": "/index.html" }
  ]
}
